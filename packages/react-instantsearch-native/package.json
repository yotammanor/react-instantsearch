    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-lodash": "3.3.2",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "enzyme": "3.3.0",
